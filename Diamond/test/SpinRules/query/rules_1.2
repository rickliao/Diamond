PREFIX ex: <http://example.com/0.1/>

[ CONSTRUCT {
  ex:s1 ex:type ex:seating .
  ex:s1 ex:seat1 ?n1 .
} WHERE {
  ?p1 ex:type ex:guest .
  ?p1 ex:name ?n1 .
} ]

[ DELETE {
  ex:s1 ex:type ex:seating .
  ex:s1 ex:seat1 ?n1 .
} WHERE {
  ?p1 ex:type ex:guest .
  ?p1 ex:name ?n1 .
  FILTER(?n1 != "Kate") .
} ]
