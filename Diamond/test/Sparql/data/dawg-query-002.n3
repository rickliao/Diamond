@prefix foaf:       <http://xmlns.com/foaf/0.1/> .
@prefix rdf:        <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:	    <http://www.w3.org/2000/01/rdf-schema#> .

<http://www.example.org/alice>
    rdf:type        foaf:Person ;
    foaf:name       "Alice" ;
    foaf:mbox       <mailto:alice@work> ;
    foaf:knows      <http://www.example.org/bob> ;
    .

<http://www.example.org/bob>
    rdf:type        foaf:Person ;
    foaf:name       "Bob" ; 
    foaf:knows      <http://www.example.org/alice> ;
    foaf:mbox       <mailto:bob@work> ;
    foaf:mbox       <mailto:bob@home> ;
    .


<http://www.example.org/eve>
    rdf:type      foaf:Person ;
    foaf:name     "Eve" ; 
    foaf:knows    <http://www.example.org/fred> ;
    .

<http://www.example.org/fred>
    rdf:type      foaf:Person ;
    foaf:mbox     <mailto:fred@edu> .
